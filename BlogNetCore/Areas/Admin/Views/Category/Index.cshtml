@model IEnumerable<Category>

<div class="row">
    <br />
    <div class="col s12">
        <a asp-action="Detail" class="btn">Create</a>
    </div>
    <br />

    <div class="col s12">
        <ul class="collapsible">
            @foreach (var c in Model)
            {
                <partial name="_TreeCategory" model="@c" />
            }
        </ul>
    </div>
</div>